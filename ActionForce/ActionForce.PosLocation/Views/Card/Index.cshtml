        function AddBasket(priceid, cardpriceid, cardnumber) {
            $.ajax({
                method: "GET",
                url: "/Card/AddBasket",
                data: { id: priceid, cardpriceid, cardnumber }
            }).done(function (d) {
                $("#BasketBody").html(d);
            });
        };



        function RemoveBasketItem(id, cardnumber) {
            $.ajax({
                method: "GET",
                url: "/Card/RemoveBasketItem",
                data: { id:id, cardnumber }
            }).done(function (d) {
                $("#BasketBody").html(d);
                $('.footer-basket').attr('style', 'display:block');
            });
        };

