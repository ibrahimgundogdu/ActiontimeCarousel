        function ClearBasket(cardnumber) {
            $.ajax({
                method: "GET",
                url: "/Card/ClearBasket",
                data: { cardnumber }
            }).done(function (d) {
                $("#BasketBody").html(d);

                $('.fixed-footer-overlay, .bot-list-btn').removeClass('open');
            });
        };


