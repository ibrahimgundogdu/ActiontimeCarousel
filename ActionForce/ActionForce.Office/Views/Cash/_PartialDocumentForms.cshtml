                        <option value="1">Maaş Ödemesi</option>
                        <option value="2">Avans Ödemesi</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Banka</label>
                    <select class="form-control" id="BankAccountID" name="BankAccountID">
                        <option value="0">Kasa</option>
                        @foreach (var item in Model.BankAccountList)
                        {
                            <option value="@item.ID">@($"{item.Bank.Name} ({item.AccountName})")</option>
                        }
                    </select>
                </div>
                <div class="form-group">
                    <label>Tutar</label>
                    <input id="Amount" name="Amount" type="text" class="form-control money" autocomplete="off" min="0" placeholder="Tutar" required>
                </div>
                <div class="form-group">
                    <label>Para Birimi</label>
                    <select class="form-control" id="Currency" name="Currency" required>
                        @foreach (var item in Model.CurrencyList)
                        {
                            <option value="@item.Code">@($"{item.Code} ({item.Sign})")</option>
                        }
                    </select>
                </div>
                <div class="form-group">
                    <label>Tarih</label>
                    <input id="DocumentDate" name="DocumentDate" type="text" class="form-control" autocomplete="off" placeholder="Tarih" value="@(DateTime.Now.ToString("yyyy-MM-dd"))" required>
                </div>
                <div class="form-group">
                    <label>Referans ID</label>
                    <input id="ReferanceID" name="ReferanceID" type="text" class="form-control" autocomplete="off" placeholder="Referans ID">
                </div>
                <div class="form-group">
                    <label>Açıklama</label>
                    <textarea id="Description" name="Description" rows="4" class="form-control" placeholder="Açıklama"></textarea>
                </div>

                <button class="btn btn-primary btn-block">Tahsilat Ekle</button>
            </div>

        }

    </div>

    

</div>
