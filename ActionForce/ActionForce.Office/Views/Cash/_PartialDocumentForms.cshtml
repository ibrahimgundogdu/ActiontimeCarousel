<div id="OffCanvasAddCashCollect" class="off-canvas off-canvas-overlay off-canvas-right wd-400" data-backdrop="static">
    <a href="#" class="close"><i data-feather="x"></i></a>

    <div class="pd-25 ht-100p tx-13">
        @using (Html.BeginForm("AddCashCollection", "Cash", FormMethod.Post))
        {
            @Html.AntiForgeryToken()
            <div>
                <h4>Yeni Kasa Tahsilatı</h4>
                <p class="tx-color-03">Kasaya nakit giriş işlemi yapar.</p>
                <input id="ActinTypeID" name="ActinTypeID" type="hidden" value="23" />
                <div class="form-group">
                    <label>Kimden</label>
                    <select class="form-control" id="FromID" name="FromID" required>
                        @foreach (var item in Model.FromList)
                        {
                            <option value="@item.Code">@(Html.Raw($"{item.Name}"))</option>
                        }
                    </select>
                </div>
                <div class="form-group">
                    <label>Lokasyon</label>
                    <select class="form-control" id="LocationID" name="LocationID" required>
                        @foreach (var item in Model.LocationList)
                        {
                            <option value="@item.LocationID">@($"{item.SortBy}  {item.LocationName} {item.State} {item.Description}" )</option>
                        }
                    </select>
                </div>
                <div class="form-group">
                    <label>Tutar</label>
                    <input id="Amount" name="Amount" autocomplete="off" type="text" class="form-control money" placeholder="Tutar" required>
                </div>
                <div class="form-group">
                    <label>Referans ID</label>
                    <input id="ReferansID" name="ReferansID" autocomplete="off" type="text" class="form-control" placeholder="Referans ID">
                </div>
                <div class="form-group">
                    <label>Para Birimi</label>
                    <select class="form-control" id="Currency" name="Currency" required>
                        @foreach (var item in Model.CurrencyList)
                        {
                            <option value="@item.Code">@($"{item.Code} ({item.Sign})")</option>
                        }
                    </select>
                </div>
                <div class="form-group">
                    <label>Tarih</label>
                    <input id="DocumentDate" name="DocumentDate" type="text" class="form-control DocumentDate" placeholder="Tarih" value="@(DateTime.Now.ToString("yyyy-MM-dd"))" required>
